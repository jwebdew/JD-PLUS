document.write('<a href="javascript:premiumAdOpen()"><img src="http://image.kyobobook.co.kr/newimages/adcenter/IMAC/creatives/2021/07/15/premium/20210715_1626323740230.jpg" width="80" height="130" border="0" alt="premium ad" /></a>'); document.writeln('<div><a href="javascript:premiumAdOpen()"><img src="http://image.kyobobook.co.kr/ink/images/gnb/btn_premium_ad.gif" alt="premium ad open" /></a></div>'); document.writeln('<div id="ad_premium" style="display:none;position:absolute;left:88px;top:0;height:290px;width:688px;border:1px solid #999;background-color:#fff;">'); document.writeln('<a href="http://imac.kyobobook.co.kr/click/INK/ink_wel_2016/ink_wel_2016@wel_03?adsno=107301&ctvno=71729&clkno=71763"><img src="http://image.kyobobook.co.kr/newimages/adcenter/IMAC/creatives/2021/07/15/premium/main/20210715_1626323740396.jpg" width="688" height="290" alt="premium ad" /></a>'); document.writeln('<a href="javascript:premiumAdClose()" style="position:absolute;left:650px;top:15px;"><img src="http://image.kyobobook.co.kr/ink/images/gnb/btn_premium_ad_close.png" alt="premium ad close" /></a>'); document.writeln('</div>'); document.writeln(''); document.writeln('<script type="text/javascript">'); document.writeln('function premiumAdOpen() {'); document.writeln('	if( !jQuery(\'#ad_premium\').is(\':animated\') ) {'); document.writeln('		if( jQuery(\'#ad_premium\').is(\':hidden\') ) {'); document.writeln('			jQuery(\'#ad_premium\').css({\'display\':\'block\',width:0,height:0}).animate({width:688,height:290},400);'); document.writeln('		} else {'); document.writeln('			premiumAdClose();'); document.writeln('		}'); document.writeln('	}'); document.writeln('}'); document.writeln('function premiumAdClose() {'); document.writeln('	jQuery(\'#ad_premium\').css({width:688,height:290}).animate({width:0,height:0},400,function(){'); document.writeln('		jQuery(this).css({\'display\':\'none\'});'); document.writeln('	});'); document.writeln('}'); document.writeln('</script>');